        "import random\n",
        "\n",
        "var = random.randint(1,100)\n",
        "print(\"出た数字は\"+str(var))\n",
        "\n",
        "if var%3 == 0:\n",
        "  print(\"Fizz\")\n",
        "elif var%5 ==0:\n",
        "  print(\"Buzz\")\n",
        "elif var%3 == 0 and var%5 ==0:\n",
        "  print(\"FizzBuzz\")\n",
        "else:\n",
        "  print(var)"
      ]
