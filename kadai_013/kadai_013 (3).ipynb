      "source": [
        "#商品の値段と消費税を定義\n",
        "def good_price(price,tax):\n",
        "\n",
        "#商品の総額の計算式\n",
        "  total = price+(price*tax/100)\n",
        "  #戻り値の設定\n",
        "  return (total)\n",
        "\n",
        "#出力したい文字列\n",
        "print(f\"合計金額は{good_price(110,10)}円です\")"
      ]
